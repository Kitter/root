{
  "process_type": "V",
  "create": [
    {
      "MODEL": "Video",
      "spec": {
        "name": "aws_video",
        "url": "s3://aub3temp/1.mp4"
      },
      "tasks": [
        {
          "operation": "perform_import",
          "video_id": "__pk__",
          "arguments": {
            "map": [
              {
                "operation": "perform_video_segmentation",
                "arguments": {
                  "map": [
                    {
                      "operation": "perform_video_decode",
                      "arguments": {
                        "segments_batch_size": 10,
                        "rate": 30,
                        "rescale": 0,
                        "map": [
                          {
                            "operation": "perform_detection",
                            "arguments": {
                              "filters": "__parent__",
                              "detector": "coco",
                              "map": [
                                {
                                  "operation": "perform_indexing",
                                  "arguments": {
                                    "index": "inception",
                                    "target": "regions",
                                    "filters": {
                                      "event_id": "__parent_event__",
                                      "w__gte": 50,
                                      "h__gte": 50
                                    }
                                  }
                                }
                              ]
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    }
  ]
}