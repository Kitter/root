{
  "name": "NYC Frame list from YFCC",
  "description": "Frame list import and processing example",
  "process_type": "V",
  "create": [
    {
      "MODEL": "Video",
      "spec": {
        "name": "NYC images",
        "dataset": "True"
      },
      "tasks": [
        {
          "operation": "perform_import",
          "video_id": "__pk__",
          "arguments": {
            "path": "https://www.dropbox.com/s/ok1rshef20ss161/framelist.gz",
            "next_tasks": [
              {
                "operation": "perform_frame_download",
                "arguments": {
                  "frames_batch_size": 100,
                  "next_tasks": [
                    {
                      "operation": "perform_detection",
                      "arguments": {
                        "filters": {
                          "event_id": "__parent_event__"
                        },
                        "detector": "textbox",
                        "target": "frames",
                        "next_tasks": [
                          {
                            "operation": "perform_analysis",
                            "arguments": {
                              "analyzer": "crnn",
                              "target": "regions",
                              "filters": {
                                "event_id": "__parent_event__"
                              }
                            }
                          }
                        ]
                      }
                    },
                    {
                      "operation": "perform_detection",
                      "arguments": {
                        "filters": {
                          "event_id": "__parent_event__"
                        },
                        "detector": "coco",
                        "next_tasks": [
                          {
                            "operation": "perform_indexing",
                            "arguments": {
                              "index": "inception",
                              "target": "regions",
                              "filters": {
                                "event_id": "__parent_event__",
                                "w__gte": 50,
                                "h__gte": 50
                              }
                            }
                          }
                        ]
                      }
                    },
                    {
                      "operation": "perform_detection",
                      "arguments": {
                        "detector": "face",
                        "filters": {
                          "event_id": "__parent_event__"
                        },
                        "next_tasks": [
                          {
                            "operation": "perform_indexing",
                            "arguments": {
                              "index": "facenet",
                              "target": "regions",
                              "filters": {
                                "event_id": "__parent_event__"
                              }
                            }
                          }
                        ]
                      }
                    },
                    {
                      "operation": "perform_indexing",
                      "arguments": {
                        "index": "inception",
                        "filters": {
                          "event_id": "__parent_event__"
                        },
                        "target": "frames"
                      }
                    },
                    {
                      "operation": "perform_analysis",
                      "arguments": {
                        "analyzer": "tagger",
                        "target": "frames",
                        "filters": {
                          "event_id": "__parent_event__"
                        }
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    }
  ]
}
