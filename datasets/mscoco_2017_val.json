{
  "name": "MSCOCO_2017_Validation",
  "description": "2017 MSCOCO validation set",
  "process_type": "V",
  "create": [
    {
      "MODEL": "Video",
      "spec": {
        "name": "2017 MSCOCO validation set",
        "url": "http://images.cocodataset.org/zips/val2017.zip",
        "dataset": true
      },
      "tasks": [
        {
          "operation": "perform_import",
          "video_id": "__pk__",
          "arguments": {
            "map": [
              {
                "operation": "perform_dataset_extraction",
                "arguments": {
                  "map": [
                    {
                      "operation": "perform_region_import",
                      "arguments": {
                        "path": "https://vdn.nyc3.digitaloceanspaces.com/datasets/coco_val2017_keypoints_regions.gz"
                      }
                    },
                    {
                      "operation": "perform_region_import",
                      "arguments": {
                        "path": "https://vdn.nyc3.digitaloceanspaces.com/datasets/coco_val2017_instances_regions.gz"
                      }
                    },
                    {
                      "operation": "perform_region_import",
                      "arguments": {
                        "path": "https://vdn.nyc3.digitaloceanspaces.com/datasets/coco_val2017_captions_regions.gz"
                      }
                    }                    
                  ]
                }
              }
            ]
          }
        }
      ]
    }
  ]
}
